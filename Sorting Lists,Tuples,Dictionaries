li=[9,1,8,2,7,3,6,4,5]
sorted(li)
print(li)
s_li=sorted(li)
print(s_li)

li=[9,1,8,2,7,3,6,4,5]
li.sort()
print(li)

li=[9,1,8,2,7,3,6,4,5]
s_li=sorted(li,reverse=True)
print(s_li)

li=[9,1,8,2,7,3,6,4,5]
li.sort(reverse=True)
print(li)

li=[-6,-5,-4,1,2,3]
s_li=sorted(li)
print(s_li)

li=[-6,-5,-4,1,2,3]
s_li=sorted(li,key=abs)
print(s_li)

tup=(9,1,8,2,7,3,6,4,5)
s_tup=sorted(tup)
print(s_tup)

dic={'name':'John','age':20,'city':'hyd'}
s_dic=sorted(dic)
print(s_dic)
