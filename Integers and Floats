 x=10
 print(type(x))
 
 #Arithmatic Operators
 print(3+2)
 print(3-2)
 print(3*2)
 print(3/2)
 print(3//2)
 print(3**2)
 print(3%2)
 print(3+2*2)
 print(3+(2*2))
 
 num=1
 num+=10
 print(num)
 print(abs(-3))
 print(round(3.75))
 print(round(3.75,1))
 
 #comparison operators
 print(3==2)
 print(3!=2)
 print(3<2)
 print(3<=2)
 print(3>2)
 print(3>=2)
 
 num_1='100'
 num_2='200'
 print(num_1+num_2)
 print(int(num_1)+int(num_2))
 
 
