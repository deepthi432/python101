print(new_str)

from my_module import hello_func,test
courses='Hi'
new_str=hello_func(courses)
print(new_str)
print(test)

from my_module import *
courses='Hi'
new_str=hello_func(courses)
print(new_str)
print(test)

from my_module import hello_func,test
import sys
courses='Hi'
new_str=hello_func(courses)
print(sys.path)

